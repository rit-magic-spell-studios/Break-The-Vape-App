                </ui:Button>
            </ui:VisualElement>
            <ui:VisualElement class="uofr-vert-list uofr-container">
                <ui:VisualElement class="uofr-section-small">
                    <ui:Label text="Total Score" class="uofr-text-small" />
                    <ui:Label text="300 points" name="TotalScoreLabel" class="uofr-text-medium" />
