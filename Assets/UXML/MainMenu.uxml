                        <ui:Label text="v0.3.0" class="uofr-text-small" style="flex-grow: 0;" />
                    </ui:Button>
                    <ui:VisualElement class="uofr-space-small" />
                    <ui:Button name="NotSoTastyButton" class="uofr-button uofr-button-discreet" style="background-color: rgb(213, 255, 202); flex-grow: 1;">
