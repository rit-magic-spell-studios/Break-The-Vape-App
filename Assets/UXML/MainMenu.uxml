                <ui:Button name="CraveSmashButton" class="uofr-button uofr-vert-list-first mm-button-game">
                    <ui:Label text="Crave Smash" class="uofr-title" style="-unity-text-align: middle-left;" />
                    <ui:Label text="v0.5.3" class="uofr-subtitle" style="-unity-text-align: middle-left;" />
                </ui:Button>
                <ui:Button name="MatchAndCatchButton" class="uofr-button uofr-vert-list-middle mm-button-game">
                    <ui:Label text="Match &amp; Catch" class="uofr-title" style="-unity-text-align: middle-left;" />
                    <ui:Label text="v0.2.2" class="uofr-subtitle" style="-unity-text-align: middle-left;" />
                </ui:Button>
                <ui:Button name="PuffDodgeButton" class="uofr-button uofr-vert-list-middle mm-button-game">
                    <ui:Label text="Puff Dodge" class="uofr-title" style="-unity-text-align: middle-left;" />
                    <ui:Label text="v0.0.0 | In Development" class="uofr-subtitle" style="-unity-text-align: middle-left;" />
                </ui:Button>
                <ui:Button name="NotSoTastyButton" class="uofr-button mm-button-game uofr-vert-list-last">
                    <ui:Label text="Not So Tasty" class="uofr-title" style="-unity-text-align: middle-left;" />
                    <ui:Label text="v0.1.0" class="uofr-subtitle" style="-unity-text-align: middle-left;" />
