                <ui:VisualElement class="uofr-space-small" />
                <ui:Label text="This will remove all user-specific data saved to this device. This include the demographic data entered at the start of the check-in form when logging in for the first time. When logging back in, you will have to re-enter this data in order to use this app." class="uofr-text-small" style="-unity-text-align: upper-center;" />
                <ui:VisualElement class="uofr-space-medium" />
                <ui:Button text="Delete User Data" name="ConfirmDeleteUserDataButton" class="uofr-button uofr-button-highlight " />
                <ui:VisualElement class="uofr-space-small" />
                <ui:Button text="Cancel" name="CancelDeleteUserDataButton" class="uofr-button " />
            </ui:VisualElement>
            <ui:VisualElement name="PlayGoalCompletePopup" class="uofr-container " style="display: none;">
                <ui:VisualElement style="background-image: url(&quot;project://database/Assets/Sprites/Icons/Big%20Smile.png?fileID=21300000&amp;guid=3a949132da27b894fad2fff1e5ca54e8&amp;type=3#Big Smile&quot;); height: 300px; -unity-background-scale-mode: scale-to-fit;" />
