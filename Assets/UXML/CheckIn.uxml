                            <engine:Button text="Boredom" class="uofr-button uofr-button-discreet" style="width: 45%; flex-grow: 0; margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px;" />
                            <engine:Button text="Other" class="uofr-button uofr-button-discreet" style="width: 45%; flex-grow: 0; margin-top: 10px; margin-right: 10px; margin-bottom: 10px; margin-left: 10px;" />
                        </engine:VisualElement>
                        <engine:VisualElement name="CravingIntensityContainer" style="flex-grow: 1; display: none; margin-top: 50px; margin-right: 0; margin-bottom: 50px; margin-left: 0;">
                            <engine:SliderInt value="3" high-value="5" name="CravingIntensitySlider" class="uofr-slider-int uofr-vert-list-first" />
